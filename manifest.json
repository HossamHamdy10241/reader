{
    "manifest_version": 3,
    "name": "My Extension",
    "version": "1.0",
    "description": "A simple browser extension.",
    "permissions": [
      "storage",
      "activeTab",
      "scripting"
    ],
    "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["<all_urls>"]
    }
  ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "open-book.png"
    
      }
    }
  }
  